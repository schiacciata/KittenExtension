{
  "name": "Cute Kittens",
  "version": "1.0.0",
  "description": "üêà An extension to see a cute kitten everytime you click on it",
  "manifest_version": 3,
  "permissions": [],
  "host_permissions": [
    "<all_urls>"
  ],
  "action": {
    "default_popup": "src/html/popup.html",
    "default_icon": {
      "16": "src/img/icons/icon16.png",
      "32": "src/img/icons/icon32.png",
      "48": "src/img/icons/icon48.png",
      "128": "src/img/icons/icon128.png"
    }
  },
  "icons": {
    "16": "src/img/icons/icon16.png",
    "32": "src/img/icons/icon32.png",
    "48": "src/img/icons/icon48.png",
    "128": "src/img/icons/icon128.png"
  },
  "options_page": "src/html/options.html"
}